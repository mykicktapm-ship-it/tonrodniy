# Server port for the Express API
PORT=4000

# Supabase project URL (https://<project>.supabase.co)
SUPABASE_URL=https://your-project.supabase.co

# Supabase service role key used for privileged backend operations
SUPABASE_SERVICE_ROLE_KEY=replace-with-service-role-key

# Secret used to sign and verify backend-issued JWTs
JWT_SECRET=replace-with-strong-jwt-secret

# Telegram bot token used to send operational alerts
TELEGRAM_BOT_TOKEN=replace-with-telegram-bot-token

# Shared secret for verifying TON webhook callbacks
TON_WEBHOOK_SECRET=replace-with-ton-webhook-secret
